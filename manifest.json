{
  "manifest_version": 2,
  "name": "PrivacyGuard Content Test",
  "version": "0.1",
  "description": "Testing content script injection.",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["js/content.js"],
      "run_at": "document_idle"
    }
  ],
  "permissions": ["<all_urls>"]
}