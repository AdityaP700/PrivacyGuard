{
  "manifest_version": 2,
  "name": "PrivacyGuard",
  "version": "1.0",
  "permissions": ["webRequest", "webRequestBlocking", "<all_urls>", "storage"],
  "background": { "scripts": ["js/background.js"] },
  "content_scripts": [{ "matches": ["<all_urls>"], "js": ["js/content.js"] }],
  "browser_action": { "default_popup": "popup/popup.html" }
}